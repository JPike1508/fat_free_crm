.remote
  - remote_form_for(@prospect) do |f|
    = link_to_close edit_prospect_path(@prospect)
    = f.hidden_field :user_id
    = f.error_messages
    = render :partial => "prospects/top_section", :locals => { :f => f, :edit => true }
    .buttonbar
      = f.submit "Save Lead"
      or
      = link_to_cancel edit_prospect_path(@prospect)
